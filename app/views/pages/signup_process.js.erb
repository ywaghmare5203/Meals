    <% if @user.errors.any? %>
      $("#error_msg").empty();
         $("#error_msg").html("<%= escape_javascript(render :partial => 'error_msg' ) %>");
     <% end %>

     <% if @user_exist %>
         $("#error_msg").empty();
         $("#error_msg").html("<%= escape_javascript("User exists allready" ) %>");

     <% end %>